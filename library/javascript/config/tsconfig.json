{
  "compilerOptions": {
    "target": "es5",
    "outDir": "../build/",
    "removeComments": true,
    "noImplicitAny": false,
    "preserveConstEnums": true,
    "sourceMap": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": "../src/",
    "lib": [
      "es5",
      "es6",
      "es2015.collection",
      "es2015.promise",
      "es2015.iterable",
      "es2016.array.include"
    ],
  },
  "include": ["../src/**/*.ts"],
  "exclude": ["../node_modules"]
}
